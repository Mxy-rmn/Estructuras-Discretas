#include<iostream>
#include<set>
using namespace std;
void mostrar(set<string> A){
  for(auto e:A)
    {
        cout<<e<<" , ";
    }
    cout<<endl;
}
int main()
{   set<string> A;
 string S;
 while(S!="SALIR"){
        mostrar(A);
    cout<<"Ingrese elemento :";
    cin>>S;
    A.insert(S);
 }
    return 0;
}
